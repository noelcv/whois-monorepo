{
  "version": 2,
  "builds": [
    {
      "src": "./index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://whois-monorepo-client.vercel.app"
      }
    }
  ],
  "rewrites": [
    {
        "source": "/(.*)",
        "destination": "/"
    }
]
}
