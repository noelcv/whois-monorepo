{
  "version": 2,
  "builds": [
    {
      "src": "./server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*noelcv.vercel.app"
      }
    }
  ]
}